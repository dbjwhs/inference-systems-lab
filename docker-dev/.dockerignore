build/
cmake-build-*/
.cmake/
CMakeCache.txt
CMakeFiles/
compile_commands.json

# Git directory (but keep .gitignore)
.git/
.gitattributes

# IDE and editor files
.vscode/
.idea/
*.swp
*.swo
*~
.DS_Store
Thumbs.db

# Temporary and cache files
*.tmp
*.temp
*.cache
__pycache__/
*.pyc
*.pyo
*.pyd
.Python
.pytest_cache/

# Log files
*.log
logs/
*.out

# Node modules (if any)
node_modules/
npm-debug.log*

# Python virtual environments
venv/
env/
.env

# Documentation build output
docs/_build/
docs/html/
docs/latex/

# Test and benchmark output
test-results/
benchmark-results/
coverage/
htmlcov/
.coverage

# Static analysis output
static_analysis_report.json
.clang-tidy-cache/

# Backup files
*.bak
*.backup
*~

# Local development files
.local/
.cache/

# OS-specific files
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Archives
*.tar
*.tar.gz
*.tar.bz2
*.zip
*.rar
*.7z

